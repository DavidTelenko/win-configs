[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[credential "helperselector"]
    selected = manager
[core]
    pager = delta
    symlinks = true
    editor = nvim
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true
[merge]
    conflictstyle = diff3
[diff]
    colorMoved = default
[init]
    defalutBranch = main
[rerere]
    enabled = true
[push]
    autoSetupRemote = true
    default = current
